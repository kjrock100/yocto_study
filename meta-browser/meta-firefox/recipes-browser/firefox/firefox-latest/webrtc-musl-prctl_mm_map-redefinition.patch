+++ ./third_party/libwebrtc/rtc_base/platform_thread_types.cc	2025-12-11 12:45:44.983639418 +0100
@@ -12,7 +12,6 @@
 
 // IWYU pragma: begin_keep
 #if defined(WEBRTC_LINUX)
-#include <linux/prctl.h>
 #include <sys/prctl.h>
 #include <sys/syscall.h>
 
